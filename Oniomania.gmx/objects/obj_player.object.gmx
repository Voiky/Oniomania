        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Movements

if( ! ( keyboard_check( ord('Z') ) || keyboard_check( ord( 'S' ) ) ) ) {

    // Stop moving with motion loss
    if( speed &gt; 1 ) {
    
        speed -= 0.25;
    
    } else if ( speed &lt; -1 ) {
    
        speed += 0.25;
    
    } else {
        speed = 0;
    }
    
    // Stop moving animation
    image_speed = 0;
    image_index = 0;

} else {
    // Moving Fowards
    if( keyboard_check( ord('Z') ) ) {
    
        speed += 0.5;    
        motion_add( direction, speed );
        
    // Moving Backwards (Doesn't work yet #broken)
    } else if ( keyboard_check( ord('S') ) ) {
        
        if( speed &gt; 0 ) {
            speed = 0;
        }
        
        x += ( ( coll_x - x ) / (33 / 2) * (-1));
        y += ( ( coll_y - y ) / (33 / 2) * (-1));
    }
    
    // Walking animation
    if( image_speed == 0 ) {
        image_speed = 1;
    }

}
        
// Player rotation
if ( keyboard_check( ord( 'Q') ) ) {

    direction += 4;
    

} else if ( keyboard_check( ord('D') ) ) {

    direction -= 4;

}

// Updating sprite angle in function of the direction ( the -90 stands for the fact that I messed up sprites by making them facing top (90 degres))
image_angle = direction -90;
    
// Clamping speed
speed = clamp(speed, -4, 4);
</string>
