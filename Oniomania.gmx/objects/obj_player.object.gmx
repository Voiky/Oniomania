        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Grabing a box

// Initialising variables
coll_offset = 33;

// Calculating collition points
switch( floor( direction / 90 ) ) {
    case 0: 
        coll_y = y - ( sqrt(power(coll_offset,2) - power( dcos(direction) * coll_offset, 2)));
        coll_x = x + ( sqrt(power(coll_offset,2) - power( dsin(direction) * coll_offset, 2))); break;
    
    case 1: 
        coll_y = y - ( sqrt(power(coll_offset,2) - power( dcos(direction) * coll_offset, 2)));
        coll_x = x - ( sqrt(power(coll_offset,2) - power( dsin(direction) * coll_offset, 2))); break;
        
    case 2: 
        coll_y = y + ( sqrt(power(coll_offset,2) - power( dcos(direction) * coll_offset, 2)));
        coll_x = x - ( sqrt(power(coll_offset,2) - power( dsin(direction) * coll_offset, 2))); break;
    
    default: 
        coll_y = y + ( sqrt(power(coll_offset,2) - power( dcos(direction) * coll_offset, 2)));
        coll_x = x + ( sqrt(power(coll_offset,2) - power( dsin(direction) * coll_offset, 2))); break;
}

// Debug pointer object
point.x = coll_x;
point.y = coll_y;

// Calculating collision with Boxes ( collision detection and keyboard detection )
if( collision_point(coll_x, coll_y, obj_box, 0, 1) != noone &amp;&amp; keyboard_check(vk_space)) {
    
    // Sprite changing
    if ( sprite_index != spr_player_holding ) { 
        sprite_index = spr_player_holding;
    }
    
    // Reassign position of the box with the collision of the player
    with (collision_point(coll_x, coll_y, obj_box, 0, 1)) {
        x = other.coll_x;
        y = other.coll_y;
        
        if (!is_transported) {
            is_transported = true;
        }
    }
    if(!use_box) {
        use_box = true;
    }
} else {
    with (collision_point(coll_x, coll_y, obj_box, 0, 1)) {
        
        if (is_transported) {
            is_transported = false;
        }
        
    }

    // Stop holding animation
    if( sprite_index != spr_player ) { 
        sprite_index = spr_player;
    }
    if(use_box) {
        use_box = false;
    }
}
</string>
          </argument>
